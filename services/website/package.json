{
  "name": "fishing-logger-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest --passWithNoTests",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "serverless deploy && serverless syncToS3 && serverless invalidateCloudFrontCache",
    "deploy:staging": "serverless deploy --stage staging && serverless syncToS3 --stage staging && serverless invalidateCloudFrontCache --stage staging",
    "deploy:prod": "serverless deploy --stage prod && serverless syncToS3 --stage prod && serverless invalidateCloudFrontCache --stage prod"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@js-joda/core": "^5.5.3",
    "@nextui-org/react": "^2.0.9",
    "bootstrap": "^5.3.1",
    "formik": "^2.4.2",
    "framer-motion": "^10.15.1",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.14.2",
    "reactstrap": "^9.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "postcss": "^8.4.27",
    "storybook": "^7.1.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
